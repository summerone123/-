                    },
                    "url": "http://www.zhihu.com/api/v4/people/36eb424c507b2f772b4d3ffff5c25824",
                    "url_token": "zhong-guo-yidai-yilu-wang",
                    "user_type": "organization",
                    "is_advertiser": false,
                    "avatar_url": "https://pic4.zhimg.com/50/v2-52ddb43fefb51e99bf573c810d891ca1_is.jpg",
                    "badge": [
                        {
                            "type": "identity",
                            "description": "已认证的官方帐号"
